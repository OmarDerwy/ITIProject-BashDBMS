#! /usr/bin/bash
shopt -s extglob

dropTableFuncSQL(){
    if [[ $# -ne 3 ]]
    then
        echo Invalid number of arguments.
    else
        if [[ $1 =~ ^[tT][aA][bB][lL][eE]+$ ]]
        then 
            if [[ -e Databases/$3/$2 ]]
            then 
                rm -r Databases/$3/$2
                echo ok
            else
                echo Syntax is $1 Table is $2  database is $3
            fi
        else
            echo Invalid syntax.
        fi
    fi
}


