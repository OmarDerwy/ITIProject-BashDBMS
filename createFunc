#! /usr/bin/bash
source ./createDatabaseFunc
source ./createTableFunc
createFunc (){
    if [[ $# -ne 3 ]]
    then
        if [[ $1 =~ ^[databaseDATABASE]+$ ]]
        then
            createDatabaseFunc ${@:2}
        elif [[ $1 = ^[tableTABLE]+$ ]]
        then
            createTableFunc ${@:2}
        fi
    else
    echo 'Invalid number of arguments.'
    fi
}